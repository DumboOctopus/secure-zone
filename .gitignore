# Ignore bundler config.
server/.bundle
server/config/secrets.yml
server/config/gc_key.json

# Ignore all logfiles and tempfiles.
server/log/*
server/tmp/*
!server/log/.keep
!server/tmp/.keep

# Ignore uploaded files in development
server/storage/*
!server/storage/.keep

server/node_modules
server/yarn-error.log

server/public/assets
server/.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key

#Ignore secrets.json file
secrets.json
server/config/master.key
